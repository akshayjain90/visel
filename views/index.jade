extends layout
block content
    div(style="text-align: center;")
        h1=title
        h2=title2

    table#table(cellpadding="10")
        tr(style="vertical-align: top;")
            td(width = 200, height =1000)
                h3="Options"
                div#options
            td(width = 1080, height =1080)
                div#workspace
                    canvas#canvas1
            td(width = 200, height =1000)
                div#mousePos
                h3="Vizualization Parameters"
                div#parameters
        tr
            td(colspan="3")
                canvas#networkData
        tr
            td(colspan="3")
                canvas#dataOnClient
        tr
            td(colspan="3")
                canvas#processorUsed
